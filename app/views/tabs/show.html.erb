<% if @tab.is_open? %>
<%= link_to '<i class="fa fa-angle-double-left" aria-hidden="true"></i>'.html_safe, root_path, class: 'navigation' %>
<% else %>
<%= link_to '<i class="fa fa-angle-double-left" aria-hidden="true"></i>'.html_safe, tabs_path, class: 'navigation' %>
<% end %>

<% if session[:user_type] == 'Customer' %>
  <%= render partial: 'table_customers' %>
<% elsif session[:user_type] == "Business" %>
  <% render partial: 'table_businesses' %>
<% end %>
